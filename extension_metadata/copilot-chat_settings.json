          "additionalProperties": {
            "type": "string"
          }
        }
      },
      "required": [
        "name",
        "url",
        "toolCalling",
        "vision",
        "maxInputTokens",
        "maxOutputTokens",
        "requiresAPIKey"
      ],
      "additionalProperties": false
    },
